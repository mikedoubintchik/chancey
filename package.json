{
  "name": "chancey",
  "description": "Best lottery app out there",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": "^8",
    "node": "^v16"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "@awesome-cordova-plugins/camera": "^5.45.0",
    "@awesome-cordova-plugins/core": "^5.45.0",
    "@awesome-cordova-plugins/geolocation": "^5.45.0",
    "@capacitor-community/fcm": "3.0.0",
    "@capacitor/android": "4.3.0",
    "@capacitor/app": "4.0.1",
    "@capacitor/camera": "^4.1.1",
    "@capacitor/core": "4.3.0",
    "@capacitor/filesystem": "^4.1.1",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/ios": "4.3.0",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/preferences": "^4.0.1",
    "@capacitor/push-notifications": "4.1.0",
    "@capacitor/status-bar": "4.0.1",
    "@capacitor/toast": "4.0.1",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@google-cloud/vision": "3.0.0",
    "@ionic-native/camera": "5.36.0",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/react": "7.0.12",
    "@ionic/react-router": "7.0.12",
    "@ionic/storage": "3.0.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "apexcharts": "3.35.5",
    "axios": "0.27.2",
    "firebase": "9.10.0",
    "ionicons": "^6.0.3",
    "lodash": "4.17.21",
    "nanoid": "4.0.0",
    "react": "18.2.0",
    "react-apexcharts": "1.4.0",
    "react-dom": "18.2.0",
    "react-joyride": "2.5.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtuoso": "2.19.0",
    "simple-statistics": "7.7.6",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "web-worker": "1.2.0",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "scripts": {
    "start": "ionic serve",
    "start:prod": "firebase serve -p 8081",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --json --passWithNoTests",
    "ci": "echo 'doing CIIIII'",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --quiet --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepush": "npm run test:ci",
    "build:ios": "ionic cap copy ios",
    "start:ios": "GENERATE_SOURCEMAP=false ionic cap run ios -l -c --external --livereload --device --open",
    "build:android": "ionic cap copy android",
    "start:android": "GENERATE_SOURCEMAP=false ionic cap run android -l --host=$(ipconfig getifaddr en0)",
    "deploy:firebase": "ionic build --prod && firebase deploy",
    "deploy:ios": "bundle exec fastlane ios",
    "deploy:ios:beta": "bundle exec fastlane beta",
    "deploy:android": "bundle exec fastlane android deploy",
    "setup": "sh scripts/setup.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "4.3.0",
    "@types/lodash": "4.14.186",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "4.3.8",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react-scripts": "5.0.1"
  }
}
